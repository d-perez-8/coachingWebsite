  - var pageTitle = 'Testimonials / reviews'
  - var pageSubtitle = 'Component is designed to showcase testimonials from satisfied clients or reviews of the products on your website.'
  - var isPageTitleBadge = true
  - var anchorNav = [{'title': 'Quotation', 'link': '#quotation'}, {'title': 'Testimonial: Style 1', 'link': '#testimonial-1'},  {'title': 'Testimonial: Style 2', 'link': '#testimonial-2'}, {'title': 'Testimonial: Style 3', 'link': '#testimonial-3'}, {'title': 'Review', 'link': '#review'}, {'title': 'Testimonials slider: Style 1', 'link': '#testimonials-slider-1'}, {'title': 'Testimonials slider: Style 2', 'link': '#testimonials-slider-2'}]
    
//- Page Content
