extends partials/layout

//- Override default config variables
append config
  - var styles = ['vendor/swiper/swiper-bundle.min.css', 'vendor/lightgallery/css/lightgallery-bundle.min.css', 'vendor/fullcalendar/main.min.css']
  - var scripts = ['vendor/swiper/swiper-bundle.min.js', 'vendor/lightgallery/lightgallery.min.js', 'vendor/lightgallery/plugins/video/lg-video.min.js', 'vendor/fullcalendar/main.min.js']
  - var parentMenu = 'Landings'
  - var pageTitle = 'Yoga Studio'
  - var navbarExtraClass = 'navbar-ignore-dark-mode ignore-dark-mode'


//- Navbar
block navbar
  include partials/navbar.pug


//- Page Content
