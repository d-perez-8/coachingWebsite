        .d-flex.d-md-block.flex-wrap.position-sticky.top-0(style='padding-top: 130px;')
          .pb-3.mb-2.mb-lg-4.me-5.me-md-0
            h2.fs-base.fw-normal.text-muted.mb-2 Company
            ul.list-unstyled.fs-lg.fw-semibold.text-dark.mb-0
              li.d-block.py-1 Vintage
              li.d-block.py-1 12-18 employees
              li.d-block.py-1 Dublin, Ireland
              li.d-block.py-1
                a(href='#').text-dark Visit website
          .pb-3.mb-2.mb-lg-4.me-5.me-md-0
            h2.fs-base.fw-normal.text-muted.mb-2 Indusrty
            ul.list-unstyled.fs-lg.fw-semibold.text-dark.mb-0
              li.d-block.py-1 E-commerce
          .pb-3.mb-2.mb-lg-4.me-5.me-md-0
            h2.fs-base.fw-normal.text-muted.mb-2 Services
            ul.list-unstyled.fs-lg.fw-semibold.text-dark.mb-0
              li.d-block.py-1 Email marketing
              li.d-block.py-1 Content marketing
              li.d-block.py-1 Social networks
