extends partials/layout

//- Override default config variables
append config
  - var styles = ['vendor/aos/dist/aos.css', 'vendor/swiper/swiper-bundle.min.css']
  - var scripts = ['vendor/aos/dist/aos.js', 'vendor/jarallax/dist/jarallax.min.js', 'vendor/swiper/swiper-bundle.min.js']
  - var parentMenu = 'Pages'
  - var pageTitle = 'Single Project v.2'
  - var buttonShape = 'rounded-0'


//- Navbar
block navbar
  include partials/navbar.pug


//- Page Content
block content

  // Hero
  section.position-relative.pt-5.mb-5
