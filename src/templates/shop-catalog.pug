      .row.align-items-center.g-0
        .col-md-6.offset-xl-1.text-center.text-md-start
          .py-4.px-4.px-sm-5.pe-md-0.ps-xl-4
            p.fs-xs.text-uppercase.pt-3.pt-md-0.mb-3.mb-lg-4 Best selling
            h2.h1.pb-2.pb-xl-3 Cozy corner for the living room at a <span class='text-primary'>discount up to 40%</span>
            a(href='#').btn.btn-sm.btn-outline-dark.ignore-dark-mode Explore
        .col-md-6.col-xl-5.d-flex.justify-content-end
          img(src='assets/img/shop/banner.jpg' width='491' alt='Banner')
    
    // Page title
    .row.pt-xl-3.mt-n1.mt-sm-0
      .col-lg-9.offset-lg-3.pt-lg-3
        h1.pb-2.pb-sm-3 Home decoration

    .row.pb-2.pb-sm-4

