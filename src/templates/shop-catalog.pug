                  span.d-inline-flex.align-items-center.text-nowrap.px-1
                    | Vases
                    i.ai-circle-x.fs-base.ms-1
                a(href='#').d-inline-block.border.rounded-pill.fs-xs.fw-medium.text-body.text-decoration-none.py-1.px-2.me-2.mb-2
