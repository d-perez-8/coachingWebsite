extends partials/layout

//- Override default config variables
append config
  - var styles = ['vendor/nouislider/dist/nouislider.min.css', 'vendor/swiper/swiper-bundle.min.css']
  - var scripts = ['vendor/nouislider/dist/nouislider.min.js', 'vendor/swiper/swiper-bundle.min.js']
  - var parentMenu = 'Pages'
  - var pageTitle = 'Shop Catalog'
  - var navbarExtraClass = 'bg-light'
  - var isShopPage = 'true'
  - var isFixedButton = true


//- Navbar
block navbar
  include partials/navbar.pug


